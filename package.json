  "scripts": {
    "clean": "rm -rf dist",
    "start": "NODE_ENV=production ts-node -O '{\"module\":\"commonjs\"}' src/server/serve.ts",
    "start:development": "NODE_ENV=development ts-node -O '{\"module\":\"commonjs\"}' src/server/serve.ts",
    "watch": "npm-run-all clean compile:relay:* --parallel watch:*",
    "watch:css": "tcm src/client/ --watch",
    "watch:client": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "watch:relay:stream": "nodemon --config .nodemon.relay-stream.json",
    "watch:server": "nodemon --config .nodemon.server.json",
    "compile:relay:stream": "NODE_PATH=./vendor ./vendor/relay-compiler/bin/relay-compiler --src ./src/client/stream --schema src/schema.graphql --language typescript --artifactDirectory ./src/client/stream/__generated__ --no-watchman"
  },
  "dependencies": {
    "@types/react-dom": "^16.0.6"
  }
